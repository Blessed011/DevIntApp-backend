        "/api/missions/delete_module/{id}": {
            "delete": {
                "description": "Удалить модуль из черновой миссии",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Миссии"
                ],
                "summary": "Удалить модуль из черновой миссии",
