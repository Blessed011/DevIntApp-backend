            "get": {
                "description": "Возвращает подробную информацию о миссии",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Миссии"
                ],
                "summary": "Получить одну миссию",
                "parameters": [
                    {
                        "type": "string",
